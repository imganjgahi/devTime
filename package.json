{
	"name": "nili-time",
	"displayName": "nili time",
	"description": "manage your active time",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extansion.setUserCongif",
		"onCommand:extansion.webViewStart",
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration":[
			{
				"type":"object",
				"title": "remoteId",
				"properties": {
					"static.conf.nili.RemoteId": {
						"type": "string",
						"default": "serverId"
					}
				}
			}
		],
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extansion.setUserCongif",
				"title": "Set Remote ID"
			},
			{
				"command": "extansion.webViewStart",
				"title": "webViewStart"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"axios": "^0.21.1"
	}
}
